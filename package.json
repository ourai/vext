{
  "name": "vext",
  "version": "0.1.0",
  "description": "Velocity for express.js",
  "homepage": "https://github.com/ourai/vext",
  "keywords": [
    "velocity",
    "vm",
    "velocityjs",
    "velocity.js",
    "express",
    "express.js",
    "node",
    "node.js"
  ],
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir src && mv index.js src/index.js",
    "build": "babel src -d . --presets=es2015",
    "prepublish": "npm run build",
    "postpublish": "rm -rf index.js && mv src/index.js index.js && rm -rf src"
  },
  "author": "Ourai Lin <ourairyu@gmail.com> (http://ourai.ws/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourai/vext.git"
  },
  "bugs": "https://github.com/ourai/vext/issues",
  "license": "MIT",
  "dependencies": {
    "velocityjs": "^0.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0"
  }
}
